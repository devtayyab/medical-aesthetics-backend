{
  "name": "medical-aesthetics-platform",
  "version": "1.0.0",
  "description": "Complete backend for Medical Aesthetics Platform",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "start:debug": "NODE_OPTIONS='--inspect' ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d src/config/data-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/config/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/config/data-source.ts",
    "db:check": "ts-node -r tsconfig-paths/register src/scripts/check-db.ts",
    "db:reset": "npm run migration:revert && npm run migration:run",
    "deploy:staging": "NODE_ENV=staging npm run migration:run && NODE_ENV=staging npm run start:prod",
    "deploy:prod": "NODE_ENV=production npm run migration:run && NODE_ENV=production npm run start:prod",
    "migration:prod": "npx typeorm migration:run -d dist/config/data-source.js"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.12",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.12",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "firebase-admin": "^13.4.0",
    "moment-timezone": "^0.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "redis": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.2.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.9.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}